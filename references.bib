	eprint = {https://doi.org/10.1509/jmr.13.0240},
	journal = {Journal of Marketing Research},
	number = {1},
	pages = {35-47},
	title = {A Probit Model with Structured Covariance for Similarity Effects and Source of Volume Calculations},
